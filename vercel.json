{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      },
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "public/index.html"
      },
      {
        "src": "/dist/(.*)",
        "dest": "dist/$1"
      }
    ]
  }